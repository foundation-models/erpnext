# ERPNext Environment Configuration
# Copy this file to .env and modify as needed

# Database Configuration
MYSQL_ROOT_PASSWORD=erpnext_root_password
MYSQL_DATABASE=erpnext
MYSQL_USER=erpnext
MYSQL_PASSWORD=erpnext_password

# ERPNext Configuration
SITE_NAME=erpnext.localhost
ADMIN_PASSWORD=admin
DEVELOPER_MODE=1
AUTO_UPDATE=0

# Redis Configuration
REDIS_CACHE_URL=redis://redis:6379
REDIS_QUEUE_URL=redis://redis:6379
REDIS_SOCKETIO_URL=redis://redis:6379

# Port Configuration
ERPNext_PORT=8000
SOCKETIO_PORT=9000
MARIADB_PORT=3306
REDIS_PORT=6379










































